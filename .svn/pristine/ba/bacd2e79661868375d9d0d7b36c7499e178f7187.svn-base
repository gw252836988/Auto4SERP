using System;
using System.Collections.Generic;



namespace Model
{
   public class SqlElement
    {

       public SqlElement()
       {

       }

       public SqlElement(string n,string v,bool iss)
       {
           name = n;
           value = v;
           isStr=iss;

       }

        private string name;

        public string Name
        {
            get { return name; }
            set { name = value; }
        }
        private string value;

        public string Value
        {
            get { return this.value; }
            set { this.value = value; }
        }

        private bool isStr;

        public bool IsStr
        {
            get { return isStr; }
            set { isStr = value; }
        }

        private bool isFuzzy;

        public bool IsFuzzy
        {
            get { return isFuzzy; }
            set { isFuzzy = value; }
        }

    }
}
